pipeline{
  agent any;
    stages{
    parallel{
      stage('Build'){
        steps {
            echo "Building is started"
            sh 'sleep 5'
            }
          }
          stages{
          parallel{
              stage('Test'){
                steps{
                  echo "Testing is started"
                  sh 'sleep 5'
                  }
                 }
                  stage('Deploy'){
                  steps {
                  echo "Deplying is started"
                  sh 'sleep 5'
                  }
              }
          }
        }
      }
}
